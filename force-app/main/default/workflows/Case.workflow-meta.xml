    <rules>
        <fullName>CAP Deficient</fullName>
        <actions>
            <name>CAP_Overdue</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>rule that checks to make sure that a case is Compliance and to see if Date Completed is Today</description>
        <formula>AND( RecordTypeName__c = &apos;Compliance Case&apos;, Due_Date_of_CAP__c &lt; TODAY() ,ISBLANK(Date_CAP_Completed__c ))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>CAP Due In 24 Hours</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeName__c</field>
            <operation>equals</operation>
            <value>Compliance Case</value>
        </criteriaItems>
        <description>Rule to be sent out 24 hours before CAP is deficient</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>CAP_Due_In_24_Hours</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Case.Due_Date_of_CAP__c</offsetFromField>
            <timeLength>-1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>CAP Due In 7 Days</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeName__c</field>
            <operation>equals</operation>
            <value>Compliance Case</value>
        </criteriaItems>
        <description>sends an email that says cap is due in 7 days</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>CAP_Due_In_7_Days</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Case.Due_Date_of_CAP__c</offsetFromField>
            <timeLength>-7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Case Owner Updated</fullName>
        <actions>
            <name>Case_Owner_Updated</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
