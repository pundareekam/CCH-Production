<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Custom_Case_Owner_Required</fullName>
    <active>true</active>
    <description>Validation created due to custom compliance page.</description>
    <errorConditionFormula>AND(RecordType.Name = &#39;Compliance Case&#39;,ISBLANK(  Case_Owner__c  ))</errorConditionFormula>
    <errorDisplayField>Case_Owner__c</errorDisplayField>
    <errorMessage>Please select Case Owner</errorMessage>
</ValidationRule>
