                            <td>
                            <apex:inputField value="{!survey.OutsideEmployer1SunEndTime__c}" label="" required="false">
                                    <apex:actionSupport event="onchange" action="{!calcTotal}" rerender="workSchedule"/>
                                </apex:inputField>
                            </td>
                        </tr>
                        </table>
                    </apex:outputPanel>    
                </apex:PageBlockSection>
                <apex:PageBlockSection columns="2" id="workSchedule">
                    <apex:outputText value="{!survey.OutsideEmployer1TotalHours__c}" label="Job 1 Total Weekly Hours:" rendered="{!survey.Is_A_Dual_Employee__c = 'Yes'}"/>  
                </apex:PageBlockSection>  

                <apex:actionRegion >
                    <apex:PageBlockSection columns="1">    
                        <apex:inputField styleClass="PageFullWidth" rendered="{!survey.Is_A_Dual_Employee__c = 'Yes'}" value="{!survey.Has_Second_Outside_Employer__c}" required="false" label="Do You Have A 2nd Outside Job?">
                            <apex:actionsupport event="onchange" rerender="secondEmployerInfo, thirdEmployerInfo, forthEmployerInfo"/>
                        </apex:inputField><br/>
                    </apex:PageBlockSection>
                </apex:actionRegion>
                
