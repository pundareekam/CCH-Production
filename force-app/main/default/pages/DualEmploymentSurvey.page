                                    <apex:actionSupport event="onchange" action="{!calcTotal}" rerender="workSchedule"/>
                                </apex:inputField>
                            </td>
                            <td>
                            <apex:inputField value="{!survey.OutsideEmployer1FriEndTime__c}" label="" required="false">
                                    <apex:actionSupport event="onchange" action="{!calcTotal}" rerender="workSchedule"/>
                                </apex:inputField>
                            </td>
                            <td>
                            <apex:inputField value="{!survey.OutsideEmployer1SatEndTime__c}" label="" required="false">
                                    <apex:actionSupport event="onchange" action="{!calcTotal}" rerender="workSchedule"/>
                                </apex:inputField>
