                        <apex:inputField styleClass="PageFullWidth" rendered="{!survey.Is_A_Dual_Employee__c != 'No' && survey.Has_Second_Outside_Employer__c = 'Yes'}" value="{!survey.Second_Outside_Employer_State__c}" required="true" label="2nd Outside Employer State?" />
                        <apex:inputField styleClass="PageFullWidth" rendered="{!survey.Is_A_Dual_Employee__c != 'No' && survey.Has_Second_Outside_Employer__c = 'Yes'}" value="{!survey.Second_Outside_Employer_Zip_Code__c}" required="false" label="2nd Outside Employer Zip Code?" />
                        <apex:inputField styleClass="PageFullWidth" rendered="{!survey.Is_A_Dual_Employee__c != 'No' && survey.Has_Second_Outside_Employer__c = 'Yes'}" value="{!survey.Second_Outside_Employer_Brief_Job__c}" required="true" label="2nd Outside Employer Brief Job Description?" />
                        <apex:inputField styleClass="PageFullWidth" rendered="{!survey.Is_A_Dual_Employee__c != 'No' && survey.Has_Second_Outside_Employer__c = 'Yes'}" value="{!survey.Second_Outside_Employer_Job_Status__c}" required="true" label="2nd Outside Employer Job Status" />
                        <apex:inputField styleClass="PageFullWidth" rendered="{!survey.Is_A_Dual_Employee__c != 'No' && survey.Has_Second_Outside_Employer__c = 'Yes'}" value="{!survey.Second_Outside_Employer_Hire_Date__c}" required="true" label="2nd Outside Employer Hire Date?" />
                    </apex:PageBlockSection>

